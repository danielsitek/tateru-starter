# editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
trim_trailing_whitespace = true
insert_final_newline = true

[*.{js,jsx,ts,tsx,vue}]
indent_size = 2
quote_type = single

[*.{json,yml,babelrc}]
indent_size = 2

[*.{twig,html,svg}]
indent_size = 4

[*.{scss,css}]
indent_size = 4
